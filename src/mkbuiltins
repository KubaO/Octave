#!/bin/sh

cat << \EOF
// DO NOT EDIT!  Generated automatically by mkbuiltins.

#ifdef HAVE_CONFIG_H
#include "config.h"
#endif

#include "builtins.h"
#include "mappers.h"
#include "oct-obj.h"
#include "variables.h"

static void
install_builtin_functions (void)
{
EOF

cat "$@"

cat << \EOF
}

void
install_builtins (void)
{
  install_builtin_variables ();
  install_mapper_functions ();
  install_builtin_functions ();
}
EOF

exit 0
